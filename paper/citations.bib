  keywords-plus =              {{STIMULATED ELECTROMAGNETIC EMISSION; IONOSPHERIC HEATING EXPERIMENTS; POWER RADIO-WAVES; OPTICAL-EMISSIONS; PARAMETRIC-INSTABILITIES; DENSITY IRREGULARITIES; ELECTRON ACCELERATION; HF; PLASMA; EXCITATION}},
  language =                   {{English}},
  number-of-cited-references = {{57}},
